const res = await fetch("url"); // fetching data you need to add url yourself
const reqData = res.filter((color) => color.name.includes("blue")); // filtering colors which contain blue

const res = await fetch("url");
const reqData = res.filter((color) => color.hexcode === "9FAC58"); // colors with given hashcode

const res = await fetch("url");
const reqData = res.filter((color) =>
  color.complementarycolor.includes("blue")
);

const res = await fetch("url");
const reqData = res.filter((color) => color.complementarycolorhex === "9FAC58");
